         defaultChecked={task.done}\n            onClick={() => checkTask(task.uuid, task.done)}\n            type=\"checkbox\"\n          />\n          {statusInput.idTask === task.uuid && statusInput.status ? (\n            <input\n              className={style.editTask}\n              autoFocus\n              onBlur={() => blurInput()}\n              onKeyDown={handleKeyPress}\n              value={userInput}\n              onChange={handleChange}\n              type=\"text\"\n            />\n          ) : (\n            <div\n              className={style.task__text}\n              uuid={task.uuid}\n              onDoubleClick={changeStatusInput}\n            >\n              {task.name}\n            </div>\n          )}\n        </section>\n        <button\n          className={style.task__btnDelete}\n          onClick={() => removeTask(task.uuid)}\n        >\n          del\n        </button>\n      </div>\n\n      <div className={style.task__date}>{task.updatedAt}</div>\n    </div>\n  );\n}\n\nexport default Task;\n","/home/user/internship/reactClone/TODO_on_React/src/components/Pagination/Pagination.js",[],[],"/home/user/internship/reactClone/TODO_on_React/src/components/SignIn/Registration.js",["88"],[],"/home/user/internship/reactClone/TODO_on_React/src/Main.js",["89","90","91","92","93","94","95"],[],"/home/user/internship/reactClone/TODO_on_React/src/components/SignIn/SignIn.js",["96","97"],[],"/home/user/internship/reactClone/TODO_on_React/src/components/SignIn/SignOut.js",[],[],{"ruleId":"98","severity":1,"message":"99","line":2,"column":35,"nodeType":"100","messageId":"101","endLine":2,"endColumn":44},{"ruleId":"102","severity":1,"message":"103","line":24,"column":6,"nodeType":"104","endLine":24,"endColumn":34,"suggestions":"105"},{"ruleId":"98","severity":1,"message":"106","line":7,"column":7,"nodeType":"100","messageId":"101","endLine":7,"endColumn":20},{"ruleId":"107","severity":1,"message":"108","line":47,"column":25,"nodeType":"109","messageId":